{
  "name": "blackat",
  "version": "1.0.0",
  "author": "Iain Kydd <iain.kydd@gmail.com>",
  "scripts": {
    "setup": "npm ci && npm run install:client",
    "install:client": "cd client && npm ci",
    "build": "npm run build:data && npm run build:client",
    "build:client": "cd client && npm run build",
    "build:data": "node import/index.js",
    "test": "concurrently \"npm run test:client\" \"npm run test:import\"",
    "test:client": "cd client && npm run test",
    "test:import": "concurrently \"npm run unit\" \"npm run lint\"",
    "watch": "jest import/**/*.test.js --watch",
    "unit": "jest import/**/*.test.js",
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint \"import/**/*.js\"",
    "prettier": "prettier \"import/**/*.js\" --write",
    "client": "cd client && npm start",
    "dev": "npm run client"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^25.5.4",
    "nock": "^12.0.3",
    "prettier": "^2.0.5"
  }
}
